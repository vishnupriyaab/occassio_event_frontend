{
  "name": "@ctrl/ngx-emoji-mart",
  "version": "9.2.0",
  "homepage": "https://ngx-emoji-mart.vercel.app",
  "repository": "scttcper/ngx-emoji-mart",
  "license": "MIT",
  "description": "Customizable Slack-like emoji picker for Angular",
  "peerDependencies": {
    "@angular/core": ">=15.0.0-0"
  },
  "exports": {
    "./picker": {
      "default": "./picker.css"
    },
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/ctrl-ngx-emoji-mart.mjs",
      "esm": "./esm2022/ctrl-ngx-emoji-mart.mjs",
      "default": "./fesm2022/ctrl-ngx-emoji-mart.mjs"
    },
    "./ngx-emoji": {
      "types": "./ngx-emoji/index.d.ts",
      "esm2022": "./esm2022/ngx-emoji/ctrl-ngx-emoji-mart-ngx-emoji.mjs",
      "esm": "./esm2022/ngx-emoji/ctrl-ngx-emoji-mart-ngx-emoji.mjs",
      "default": "./fesm2022/ctrl-ngx-emoji-mart-ngx-emoji.mjs"
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "angular",
    "emoji",
    "picker"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "module": "fesm2022/ctrl-ngx-emoji-mart.mjs",
  "typings": "index.d.ts",
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
